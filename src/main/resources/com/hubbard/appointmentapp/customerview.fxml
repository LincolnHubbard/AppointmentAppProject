<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hubbard.appointmentapp.controller.CustomerViewController">
    <children>
        <TableView fx:id="customerTableView" layoutX="36.0" layoutY="32.0" prefHeight="200.0" prefWidth="778.0">
            <columns>
                <TableColumn fx:id="idColumn" prefWidth="43.0" text="ID" />
                <TableColumn fx:id="nameColumn" prefWidth="131.0" text="Name" />
                <TableColumn fx:id="addrColumn" prefWidth="159.0" text="Address" />
                <TableColumn fx:id="postColumn" prefWidth="100.0" text="Postal Code" />
                <TableColumn fx:id="phoneColumn" prefWidth="134.0" text="Phone #" />
                <TableColumn fx:id="divColumn" prefWidth="113.0" text="State/Province ID" />
                <TableColumn fx:id="countryColumn" prefWidth="97.0" text="Country" />
            </columns>
        </TableView>
        <Button layoutX="36.0" layoutY="243.0" mnemonicParsing="false" onAction="#addButtonPressed" text="Add" />
        <Button layoutX="88.0" layoutY="243.0" mnemonicParsing="false" onAction="#updateButtonPressed" text="Update" />
        <Button layoutX="156.0" layoutY="243.0" mnemonicParsing="false" onAction="#deleteButtonPressed" text="Delete" />
        <Button fx:id="cancelButton" layoutX="769.0" layoutY="243.0" mnemonicParsing="false" onAction="#cancelButtonPressed" text="Close" />
        <Label layoutX="380.0" layoutY="6.0" text="View Customers">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
